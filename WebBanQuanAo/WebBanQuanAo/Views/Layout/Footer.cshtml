<script>
    function initialize() {
        var myLatlng = new google.maps.LatLng(10.761373, 106.682243);
        var mapOptions = {
            zoom: 9,
            center: myLatlng
        };

        var map = new google.maps.Map(document.getElementById('map_canvas'), mapOptions);

        var contentString = "<table><tr><th>Trường Đại học Sư Phạm TP. HCM</th></tr><tr><td>Địa chỉ: 280 An Dương Vương, Phường 4, Quận 5, Hồ Chí Minh 720000</td></tr></table>";

        var infowindow = new google.maps.InfoWindow({
            content: contentString
        });

        var marker = new google.maps.Marker({
            position: myLatlng,
            map: map,
            title: 'Trường Đại học Sư Phạm TP. HCM'
        });
        infowindow.open(map, marker);
    }

    google.maps.event.addDomListener(window, 'load', initialize);


</script>
